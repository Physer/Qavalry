{
  "name": "qavalry",
  "version": "1.0.0",
  "description": "Framework for automated testing using Cucumber, WebdriverIO and Selenium",
  "main": "index.js",
  "scripts": {
    "build": "babel-node ./src/index.js --presets @babel/preset-env node -e \"require('./src/index')\" --options ./test/options.js --config ./test/firefox.conf.js",
    "prepublish": "babel ./src --presets @babel/preset-env --out-dir dist"
  },
  "author": "Alex Schouls",
  "contributors": [
    {
      "name": "Bob Sanders"
    },
    {
      "name": "Guido van Tricht"
    }
  ],
  "repository": "github:Physer/Qavalry",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@wdio/cli": "^5.16.13",
    "@wdio/cucumber-framework": "^5.16.13",
    "@wdio/local-runner": "^5.16.13",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.16.12",
    "chai": "^4.2.0",
    "chromedriver": "^78.0.1",
    "dateformat": "^3.0.3",
    "file-system": "^2.2.2",
    "wdio-chromedriver-service": "^5.0.2",
    "wdio-cucumberjs-json-reporter": "^1.1.2"
  },
  "bin": {
    "qavalry": "./cli.js",
    "vtf": "./cli.js"
  },
  "keywords": [
    "testing",
    "webdriverio",
    "cucumber",
    "automated",
    "automation",
    "selenium"
  ]
}
