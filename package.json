{
  "name": "qavalry",
  "version": "1.0.0",
  "description": "Framework for automated testing using Cucumber, WebdriverIO and Selenium",
  "main": "index.js",
  "scripts": {
    "build": "babel-node ./src/index.js --presets @babel/preset-env node -e \"require('./src/index')\" --options ./test/options.js --config ./test/firefox.conf.js",
    "prepublish": "babel ./src --presets @babel/preset-env --out-dir dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "Alex Schouls",
  "contributors": [
    {
      "name": "Bob Sanders"
    },
    {
      "name": "Guido van Tricht"
    }
  ],
  "repository": "github:Physer/Qavalry",
  "license": "MIT",
  "dependencies": {
    "@wdio/cli": "^5.11.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "cucumber-html-reporter": "^5.0.2",
    "dateformat": "^3.0.3",
    "file-system": "^2.2.2"
  },
  "bin": {
    "qavalry": "./cli.js",
    "vtf": "./cli.js"
  },
  "keywords": [
    "testing",
    "webdriverio",
    "cucumber",
    "automated",
    "automation",
    "selenium"
  ]
}
