{
  "name": "qavalry",
  "version": "1.0.0",
  "description": "Framework for automated testing using Cucumber, WebdriverIO and Selenium",
  "main": "index.js",
  "scripts": {
    "build": "babel-node ./src/index.js --presets @babel/preset-env node -e \"require('./src/index')\" --options ./test/options.js --config ./test/firefox.conf.js",
    "prepublish": "babel ./src --presets @babel/preset-env --out-dir dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "Alex Schouls",
  "contributors": [
    {
      "name": "Bob Sanders"
    },
    {
      "name": "Guido van Tricht"
    }
  ],
  "repository": "github:Physer/Qavalry",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@wdio/cli": "^5.11.2",
    "@wdio/cucumber-framework": "^5.11.3",
    "@wdio/local-runner": "^5.11.2",
    "@wdio/selenium-standalone-service": "^5.11.2",
    "@wdio/spec-reporter": "^5.11.0",
    "allure-commandline": "^2.9.0",
    "ansi-colors": "^4.1.1",
    "chai": "^4.2.0",
    "cucumber-html-reporter": "^5.0.0",
    "cucumber-junit": "^1.7.1",
    "dateformat": "^3.0.3",
    "fancy-log": "^1.3.3",
    "wdio-allure-reporter": "^0.8.3",
    "wdio-json-reporter": "^1.2.0",
    "yargs": "^13.2.4"
  },
  "bin": {
    "qavalry": "./cli.js",
    "vtf": "./cli.js"
  },
  "keywords": [
    "testing",
    "webdriverio",
    "cucumber",
    "automated",
    "automation",
    "selenium"
  ]
}
